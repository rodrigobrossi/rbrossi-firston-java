<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="br.com.crm.db.CrmProduto" schema="dbo" table="CRM_PRODUTO">
<id column="PRD_ID" name="id" type="java.lang.Long">
<generator class="native"/>
</id>
<property column="PRD_TITULO" length="256" name="prdTitulo" not-null="true" type="java.lang.String"/>
<property column="PRD_DESCRICAO" length="256" name="prdDescricao" type="java.lang.String"/>
<property column="PRD_PRECO_UNIDADE" length="256" name="prdPrecoUnidade" type="java.lang.String"/>
</class>
</hibernate-mapping>
