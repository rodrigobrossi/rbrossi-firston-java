<property column="EST_TITULO" length="256" name="estTitulo" type="java.lang.String"/>
<property column="EST_OSERVACAO" length="256" name="estOservacao" type="java.lang.String"/>
<property column="EST_DICA_RELACIONAMENTO" length="256" name="estDicaRelacionamento" type="java.lang.String"/>
</class>
</hibernate-mapping>
