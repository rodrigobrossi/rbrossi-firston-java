<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="br.com.crm.db.CrmHistoricoContato" schema="dbo" table="CRM_HISTORICO_CONTATO">
<id column="HIS_ID" name="id" type="java.lang.Long">
<generator class="native"/>
</id>
<property column="CNT_ID_FK" name="cntIdFk" not-null="true" type="java.lang.Integer"/>
<property column="USR_ID_FK" name="usrIdFk" not-null="true" type="java.lang.Integer"/>
<property column="HIS_RESUMO" length="256" name="hisResumo" type="java.lang.String"/>
<property column="HIS_DATA" length="10" name="hisData" type="java.lang.String"/>
<property column="HIS_PENDENTE" length="256" name="hisPendente" type="java.lang.String"/>
<property column="HIS_ALERTA" length="256" name="hisAlerta" type="java.lang.String"/>
<property column="HIS_FEEDBACK_USUARIO" length="256" name="hisFeedbackUsuario" type="java.lang.String"/>
</class>
</hibernate-mapping>
