<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="br.com.crm.db.CrmEmpresas" schema="dbo" table="CRM_EMPRESAS">
<id column="EMP_ID" name="id" type="java.lang.Long">
<generator class="native"/>
</id>
<property column="EMP_NOME" length="15" name="empNome" not-null="true" type="java.lang.String"/>
<property column="EMP_CAIXA_POSTAL" length="256" name="empCaixaPostal" type="java.lang.String"/>
<property column="EMP_DDD" length="3" name="empDdd" type="java.lang.String"/>
<property column="EMP_TELEFONE1" length="8" name="empTelefone1" type="java.lang.String"/>
<property column="EMP_TELEFONE2" length="8" name="empTelefone2" type="java.lang.String"/>
<property column="EMP_FAX" length="8" name="empFax" type="java.lang.String"/>
<property column="EMP_EMAIL" length="30" name="empEmail" type="java.lang.String"/>
<property column="EMP_WEBSITE" length="30" name="empWebsite" type="java.lang.String"/>
<property column="EMP_CONDICAO_PAGAMENTO" length="50" name="empCondicaoPagamento" type="java.lang.String"/>
<property column="EMP_OBSERVACAO" length="256" name="empObservacao" type="java.lang.String"/>
<property column="EMP_NUM_BENEFCIARIOS" length="3" name="empNumBenefciarios" type="java.lang.String"/>
<property column="EMP_PRODUTO" length="256" name="empProduto" type="java.lang.String"/>
<property column="EMP_DATA_CONTRATO" length="10" name="empDataContrato" type="java.lang.String"/>
<property column="EMP_BENEFICICIOS_AGREGADOS" length="256" name="empBeneficiciosAgregados" type="java.lang.String"/>
<property column="EMP_PRECO" length="10" name="empPreco" type="java.lang.String"/>
<property column="EMP_SUBSIDIA_PARA_FUNCIONARIO" length="10" name="empSubsidiaParaFuncionario" type="java.lang.String"/>
<property column="EMP_VALOR_FATURA" length="10" name="empValorFatura" type="java.lang.String"/>
<property column="EMP_QUALIFICACAO_MARGEM" length="256" name="empQualificacaoMargem" type="java.lang.String"/>
<property column="EMP_DATA_ULTIMO_REAJUSTE" length="10" name="empDataUltimoReajuste" type="java.lang.String"/>
<property column="EMP_PORCENTAGEM_ULTIMO_REAJUSTE" length="3" name="empPorcentagemUltimoReajuste" type="java.lang.String"/>
</class>
</hibernate-mapping>
