<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="br.com.crm.db.CrmNetos" schema="dbo" table="CRM_NETOS">
<id column="NET_ID" name="id" type="java.lang.Long">
<generator class="native"/>
</id>
<property column="CNT_ID_FK" name="cntIdFk" not-null="true" type="java.lang.Integer"/>
<property column="NET_NOME" length="256" name="netNome" type="java.lang.String"/>
<property column="NET_DATA_ANIVERSARIO" length="10" name="netDataAniversario" type="java.lang.String"/>
<property column="NET_IDADE" length="2" name="netIdade" type="java.lang.String"/>
</class>
</hibernate-mapping>
