<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="br.com.crm.db.CrmProvidencias" schema="dbo" table="CRM_PROVIDENCIAS">
<id column="PRV_ID" name="id" type="java.lang.Long">
<generator class="native"/>
</id>
<property column="USR_ID_FK" name="usrIdFk" not-null="true" type="java.lang.Integer"/>
<property column="CNT_ID_FK" name="cntIdFk" not-null="true" type="java.lang.Integer"/>
<property column="PRV_PROVIDENCIAR" length="256" name="prvProvidenciar" type="java.lang.String"/>
<property column="PRV_PARA_QUANDO" length="10" name="prvParaQuando" type="java.lang.String"/>
<property column="PRV_QUEM_DEVE_PROVIDENCIAR" length="256" name="prvQuemDeveProvidenciar" type="java.lang.String"/>
<property column="PRV_QUEM_ESTA_NOTANDO" length="256" name="prvQuemEstaNotando" type="java.lang.String"/>
<property column="PRV_QUEM_ESTA_AGUARDANDO" length="256" name="prvQuemEstaAguardando" type="java.lang.String"/>
</class>
</hibernate-mapping>
